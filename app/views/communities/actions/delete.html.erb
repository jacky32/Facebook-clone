<% community = Community.find(@id)%>
<div class="user-heading">Are you want to delete the community?</div>
<div class="buttons">
  <%= button_to "Delete", community_path(community), method: :delete, class: "button delete", method: :delete, data: { controller: "modal", action: "click->modal#hideModal" } %>
  <div class="button cancel" data-controller="modal" data-action="click->modal#hideModal">Cancel</div>
</div>
